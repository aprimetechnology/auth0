echo -e "<%= env %> - Updating Auth0 custom text for mfa-webauthn prompt..."
curl -L -X PUT '<%= auth0Url %>/api/v2/prompts/mfa-webauthn/custom-text/en' \
-H 'Content-Type: application/json' \
-H 'Accept: application/json' \
-H 'Authorization: Bearer <%= auth0AuthToken %>' \
-d @dist/<%= env %>/prompt_mfa-webauthn.json
echo -e "\n"
