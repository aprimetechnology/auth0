echo -e "<%= env %> - Updating Auth0 branding theme..."
curl -L -X PATCH '<%= auth0Url %>/api/v2/branding/themes/<%= auth0ThemeId %>' \
-H 'Content-Type: application/json' \
-H 'Accept: application/json' \
-H 'Authorization: Bearer <%= auth0AuthToken %>' \
-d @dist/<%= env %>/branding_theme.json
echo -e "\n"
