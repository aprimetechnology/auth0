echo -e "<%= env %> - Updating Auth0 custom text for reset-password prompt..."
curl -L -X PUT '<%= auth0Url %>/api/v2/prompts/reset-password/custom-text/en' \
-H 'Content-Type: application/json' \
-H 'Accept: application/json' \
-H 'Authorization: Bearer <%= auth0AuthToken %>' \
-d @dist/<%= env %>/prompt_reset-password.json
echo -e "\n"
