{
  "colors": {
    "primary_button": "#000000",
    "primary_button_label": "#000000",
    "secondary_button_border": "#000000",
    "secondary_button_label": "#000000",
    "base_focus_color": "#000000",
    "base_hover_color": "#000000",
    "links_focused_components": "#000000",
    "header": "#000000",
    "body_text": "#000000",
    "widget_background": "#000000",
    "widget_border": "#000000",
    "input_labels_placeholders": "#000000",
    "input_filled_text": "#000000",
    "input_border": "#000000",
    "input_background": "#000000",
    "icons": "#000000",
    "error": "#000000",
    "success": "#000000"
  },
  "fonts": {
    "font_url": "https://fonts.com/Matter-Regular.woff2",
    "reference_text_size": 18,
    "title": {
      "bold": true,
      "size": 100
    },
    "subtitle": {
      "bold": true,
      "size": 87.5
    },
    "body_text": {
      "bold": true,
      "size": 87.5
    },
    "buttons_text": {
      "bold": true,
      "size": 125
    },
    "input_labels": {
      "bold": true,
      "size": 100
    },
    "links": {
      "bold": true,
      "size": 87.5
    },
    "links_style": "normal"
  },
  "borders": {
    "button_border_weight": 1,
    "buttons_style": "pill",
    "button_border_radius": 10,
    "input_border_weight": 1,
    "inputs_style": "pill",
    "input_border_radius": 3,
    "widget_corner_radius": 50,
    "widget_border_weight": 0,
    "show_widget_shadow": true
  },
  "widget": {
    "logo_position": "center",
    "logo_url": "<%= appUrl %>/assets/images/image.png",
    "logo_height": 100,
    "header_text_alignment": "center",
    "social_buttons_layout": "bottom"
  },
  "page_background": {
    "page_layout": "center",
    "background_color": "#000000",
    "background_image_url": ""
  }
}
